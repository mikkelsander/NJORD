<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/activity_test_lungs"
    android:layout_width="match_parent"
    tools:context="com.project.ms.njord.activity.TestLungsActivity"
    android:background="@color/dark_gray"
    android:layout_height="match_parent">


    <Button
        android:text="See Results"
        android:layout_width="139dp"
        android:layout_height="wrap_content"
        android:id="@+id/testLungs_done_button"
        android:background="@drawable/button_blue"
        android:textColor="@color/white"


        tools:layout_constraintRight_creator="1"
        tools:layout_constraintBottom_creator="1"
        app:layout_constraintBottom_toBottomOf="parent"
        android:layout_marginEnd="16dp"
        app:layout_constraintRight_toRightOf="parent"
        android:layout_marginBottom="40dp"
        android:layout_marginStart="16dp"
        app:layout_constraintLeft_toLeftOf="parent" />

    <TextView
        android:text="67"
        android:textSize="70dp"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/testLungs_lungLevel_textView"
        android:layout_marginStart="64dp"
        app:layout_constraintLeft_toLeftOf="parent"
        android:layout_marginTop="96dp"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:text="TextView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/testLungs_instructions_textView"
        app:layout_constraintTop_toBottomOf="@+id/testLungs_lungLevel_textView"
        android:layout_marginTop="80dp"
        app:layout_constraintHorizontal_bias="0.21"
        app:layout_constraintLeft_toLeftOf="@+id/testLungs_lungLevel_textView" />


</android.support.constraint.ConstraintLayout>
